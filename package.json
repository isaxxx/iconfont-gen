{
  "name": "iconfont-gen",
  "version": "1.1.1",
  "description": "Generate Iconfont from SVG files.",
  "main": "./index.js",
  "bin": {
    "iconfont-gen": "./bin/iconfont-gen"
  },
  "scripts": {
    "test": "ava --verbose",
    "check": "eslint .",
    "git-ignore": "git rm -r --cached . && git add -A && git commit --allow-empty -m 'gitignore-refresh' && git reset --soft HEAD^",
    "git-commit": "git add -A && git commit",
    "git-push": "git push origin master",
    "npm-publish": "npm publish"
  },
  "keywords": [
    "iconfont",
    "svg"
  ],
  "engines": {
    "node": ">=6.11.4"
  },
  "author": "isaxxx",
  "repository": {
    "type": "git",
    "url": "https://github.com/isaxxx/iconfont-gen"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.1",
    "ejs": "^2.5.9",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "mkdirp": "^0.5.1",
    "path": "^0.12.7",
    "svg2ttf": "^4.1.0",
    "svgicons2svgfont": "^9.0.2",
    "ttf2woff": "^2.0.1",
    "ttf2woff2": "^2.0.3",
    "update-notifier": "^2.5.0",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "eslint": "^5.3.0"
  }
}
